map {
  pattern = priv/quota/storage
  table = quota2
  username_field = username
  value_field = bytes
}

map {
  pattern = priv/quota/messages
  table = quota2
  username_field = username
  value_field = messages
}

# We require /etc/dovecot/secure/connect.conf to be present.
!include /etc/dovecot/secure/connect.conf
